{
    "version" :2,
    "builds":[
        {
        "src": "server.js",
        "use": "@versel/node",
        "config" :{
            "includesFiles":[
                "dist/**"
            ]
        }
        
        }
    ],
    "routes":[
        {
            "src": "/(.*)",
            "dest": "/server.js"
        }
    ]
}